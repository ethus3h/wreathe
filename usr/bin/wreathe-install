#!/usr/bin/env bash
# shellcheck disable=SC1091
source ember_bash_setup &> /dev/null
#set -x

trap 'die "A fatal error was reported on ${BASH_SOURCE[0]} line ${LINENO}."' ERR

ia configure
wget https://archive.org/details/Wreathe7.3/foo
partition=read
dd if=foo of="$partition"
wget https://archive.org/download/Wreathe7.3/boot.dd
bootPartition=read
dd if=foo of="$bootPartition"
